{
  "name": "trao-judge-frontend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint '**/*.{js,mjs,cjs,ts,vue}' './*.js' './*.ts'",
    "lint:debug": "eslint ./src --debug",
    "lint:fix": "eslint ./src --fix",
    "format": "prettier --write src/",
    "openapi": "openapi-typescript https://raw.githubusercontent.com/traP-jp/traO-Judge-docs/refs/tags/1.1.0/api/backend/frontend-backend.yaml --output ./src/api/openapi.ts"
  },
  "dependencies": {
    "@types/eslint__js": "^8.42.3",
    "eslint-config-prettier": "^9.1.0",
    "vue": "^3.5.6",
    "vue-router": "^4.3.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.0",
    "@rushstack/eslint-patch": "^1.8.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.14.5",
    "@vitejs/plugin-vue": "^5.1.3",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.1",
    "eslint-plugin-tailwindcss": "^3.17.4",
    "eslint-plugin-vue": "^9.28.0",
    "globals": "^15.9.0",
    "npm-run-all2": "^6.2.0",
    "openapi-typescript": "^7.4.1",
    "postcss": "^8.4.44",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.10",
    "typescript": "~5.4.0",
    "typescript-eslint": "^8.6.0",
    "vite": "^5.4.3",
    "vue-tsc": "^2.0.21"
  }
}
